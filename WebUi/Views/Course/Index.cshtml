@model CourseListViewModel
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

@foreach(var course in Model.Courses)
{
    <tr>
         <td>
            @course.Id
        </td>
        <td>
            @course.CourseName
        </td>
          <td>
            @course.CourseDescription
        </td>  <td>
            @course.Images
        </td> 
    </tr>
}